<FilesMatch "\.(html|htm)$">
    Header set Cache-Control "max-age=60, must-revalidate"
</FilesMatch>

<FilesMatch "\.(css|js)$">
    Header set Cache-Control "max-age=300, must-revalidate"
</FilesMatch>

<FilesMatch "\.(png|jpg|jpeg|gif|svg|ico)$">
    Header set Cache-Control "max-age=3600, public"
</FilesMatch>